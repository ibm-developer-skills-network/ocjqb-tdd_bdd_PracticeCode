{
    "name": "test-fixtures-lab",
    "version": "1.0.0",
    "description": "Lab for learning test fixtures in JavaScript",
    "main": "index.js",
    "scripts": {
      "test": "jest --coverage"
    },
    "dependencies": {
      "sequelize": "^6.31.1",
      "sqlite3": "^5.1.6"
    },
    "devDependencies": {
      "jest": "^29.5.0"
    },
    "jest": {
      "testEnvironment": "node",
      "verbose": true,
      "collectCoverage": true,
      "coverageDirectory": "coverage",
      "coverageReporters": ["text", "lcov"],
      "coveragePathIgnorePatterns": [
        "/node_modules/"
      ]
    }
  }